  "query": "\n            SELECT\n    d.id,\n    d.url,\n    d.total_bytes,\n    d.status,\n    d.created_at,\n    d.chunk_count,\n    d.file_path,\n    d.file_name,\n    d.content_type,\n    d.extension,\n    d.auth,\n    d.proxy,\n    d.headers,\n    d.cookies,\n    d.speed_limit,\n    d.max_retries,\n    d.delay_secs,\n    d.backoff_factor,\n    d.timeout_secs,\n    COALESCE(\n\t\t(\n\t\t\tSELECT\n\t\t\t\tSUM(c.downloaded_bytes)\n\t\t\tFROM\n\t\t\t\tdownload_chunks c\n\t\t\tWHERE\n\t\t\t\tc.download_id = d.id\n\t\t),\n\t\t0\n\t) AS downloaded_bytes\nFROM downloads d\nLEFT JOIN download_chunks c ON c.download_id = d.id\nGROUP BY d.id\nORDER BY d.created_at DESC;\n        ",
